<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.che_v_kino">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.che_v_kino"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.example.che_v_kino.newtheatre"
            android:exported="false" />
        <activity
            android:name="com.example.che_v_kino.newfilm"
            android:exported="false" />
        <activity
            android:name="com.example.che_v_kino.films"
            android:exported="false" />
        <activity
            android:name="com.example.che_v_kino.theatre"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.che_v_kino.MainActivity" />
        </activity>
        <activity
            android:name="com.example.che_v_kino.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>